INCL = -I$(CATEGORIES_HPP_DIR) -I$(CATEGORY_HPP_DIR) -I$(CATEGORYMEMBER_HPP_DIR) -I$(CATEGORYMEMBERS_HPP_DIR) -I$(COMPARE_HPP_DIR) -I$(CREATEACCOUNT_HPP_DIR) -I$(CURLWRAPPER_HPP_DIR) -I$(EDIT_HPP_DIR) -I$(JSON11_HPP_DIR) -I$(LOGEVENT_HPP_DIR) -I$(LOGEVENTS_HPP_DIR) -I$(LOGININFO_HPP_DIR) -I$(LOGEVENTPARAMS_HPP_DIR) -I$(MEDIAWIKIACTIONAPI_HPP_DIR) -I$(MEDIAWIKIACTIONAPIPARAMETERS_HPP_DIR) -I$(NEWSWIKIRU_HPP_DIR) -I$(PAGE_HPP_DIR) -I $(PROJECTSNEWSFEEDS_HPP_DIR) -I$(PURGE_HPP_DIR) -I$(REVISION_HPP_DIR) -I$(REVISIONS_HPP_DIR) -I$(REVISIONSTRACKER_HPP_DIR) -I$(ROLLBACK_HPP_DIR) -I$(SEARCH_HPP_DIR) -I$(SEARCHITEM_HPP_DIR) -I$(TOKENS_HPP_DIR) -I$(WELCOME_HPP_DIR) -I$(WIKIMEDIAPROJECT_HPP_DIR) -I$(WIKIMEDIAPROJECTS_HPP_DIR)

OBJ =  $(ACTIVITY_O) $(JSON11_O) $(INSERTSERVICEPAGE_O) $(GENERATORCATEGORYBYYEAR_O) $(PROJECTSNEWSFEEDS_O) $(REVISIONSTRACKER_O) $(SORTCATEGORIES_O) $(WELCOME_O) $(WIKICONSOLE_O)

BIN = $(ACTIVITY) $(INSERTSERVICEPAGE) $(GENERATORCATEGORYBYYEAR) $(PROJECTSNEWSFEEDS) $(REVISIONSTRACKER) $(SORTCATEGORIES) $(WELCOME) $(WIKICONSOLE)



all: $(BIN)



$(ACTIVITY) : $(JSON11_O) $(ACTIVITY_O)
	$(CC) -o $(ACTIVITY) $(LDLIBS)  $(JSON11_O) $(ACTIVITY_O)

$(GENERATORCATEGORYBYYEAR) : $(JSON11_O) $(GENERATORCATEGORYBYYEAR_O)
	$(CC) -o $(GENERATORCATEGORYBYYEAR) $(LDLIBS) $(JSON11_O) $(GENERATORCATEGORYBYYEAR_O)

$(INSERTSERVICEPAGE) : $(JSON11_O) $(INSERTSERVICEPAGE_O)
	$(CC) -o $(INSERTSERVICEPAGE) $(LDLIBS) $(JSON11_O) $(INSERTSERVICEPAGE_O)

$(PROJECTSNEWSFEEDS) : $(JSON11_O) $(PROJECTSNEWSFEEDS_O)
	$(CC) -o $(PROJECTSNEWSFEEDS) $(LDLIBS)  $(JSON11_O) $(PROJECTSNEWSFEEDS_O)

$(REVISIONSTRACKER) : $(JSON11_O) $(REVISIONSTRACKER_O)
	$(CC) -o $(REVISIONSTRACKER) $(LDLIBS)  $(JSON11_O) $(REVISIONSTRACKER_O)

$(SORTCATEGORIES) : $(JSON11_O) $(SORTCATEGORIES_O)
	$(CC) -o $(SORTCATEGORIES) $(LDLIBS) $(JSON11_O) $(SORTCATEGORIES_O)

$(WELCOME) : $(JSON11_O) $(WELCOME_O)
	$(CC) -o $(WELCOME) $(LDLIBS) $(JSON11_O) $(WELCOME_O)

$(WIKICONSOLE) : $(JSON11_O) $(WIKICONSOLE_O)
	$(CC) -o $(WIKICONSOLE) $(LDLIBS) $(JSON11_O) $(WIKICONSOLE_O)



$(ACTIVITY_O) : $(ACTIVITY_CPP) $(HEADERS)
	$(CC) $(CFLAGS) -o $(ACTIVITY_O) $(ACTIVITY_CPP) $(INCL)

$(GENERATORCATEGORYBYYEAR_O) : $(GENERATORCATEGORYBYYEAR_CPP) $(HEADERS)
	$(CC) $(CFLAGS) -o $(GENERATORCATEGORYBYYEAR_O) $(GENERATORCATEGORYBYYEAR_CPP) $(INCL)

$(INSERTSERVICEPAGE_O) : $(INSERTSERVICEPAGE_CPP) $(HEADERS)
	$(CC) $(CFLAGS) -o $(INSERTSERVICEPAGE_O) $(INSERTSERVICEPAGE_CPP) $(INCL)

$(PROJECTSNEWSFEEDS_O) : $(PROJECTSNEWSFEEDS_CPP) $(HEADERS)
	$(CC) $(CFLAGS) -o $(PROJECTSNEWSFEEDS_O) $(PROJECTSNEWSFEEDS_CPP) $(INCL)

$(REVISIONSTRACKER_O) : $(REVISIONSTRACKER_CPP) $(HEADERS)
	$(CC) $(CFLAGS) -o $(REVISIONSTRACKER_O) $(REVISIONSTRACKER_CPP) $(INCL)

$(SORTCATEGORIES_O) : $(SORTCATEGORIES_CPP) $(HEADERS)
	$(CC) $(CFLAGS) -o $(SORTCATEGORIES_O) $(SORTCATEGORIES_CPP) $(INCL)

$(WELCOME_O) : $(WELCOME_CPP) $(HEADERS)
	$(CC) $(CFLAGS) -o $(WELCOME_O) $(WELCOME_CPP) $(INCL)

$(WIKICONSOLE_O) : $(WIKICONSOLE_CPP) $(HEADERS)
	$(CC) $(CFLAGS) -o $(WIKICONSOLE_O) $(WIKICONSOLE_CPP) $(INCL)



$(JSON11_O) : $(JSON11_CPP) $(JSON11_HPP)
	$(CC) $(CFLAGS) -o $(JSON11_O) -I$(JSON11_HPP_DIR) $(JSON11_CPP)



clean:
	rm -rf $(BIN) $(OBJ)
 
